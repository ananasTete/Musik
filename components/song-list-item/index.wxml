<!--components/song-list-item/index.wxml-->
<view class="item"  bindtap="onSongItemClick">
  <view class="index">{{index}}</view>
  <view class="info">
    <view class="name">{{item.name}}</view>
    <view class="source">
      <image class="icon" src="/assets/images/icons/sq_icon.png"></image>
      <text>{{item.ar[0].name || item.artists[0].name}}  {{item.alia[0] || item.alias[0]}}</text>
    </view>
  </view>
</view>